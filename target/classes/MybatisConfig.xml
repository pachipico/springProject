		<typeAlias type="com.springprj.www.security.UserVO"
			alias="mvo" />
		<typeAlias type="com.springprj.www.security.AuthVO"
			alias="authVO" />

		
		<typeAlias type="com.springprj.www.domain.ReviewVO"
			alias="rvvo" />

		<typeAlias type="com.springprj.www.domain.RatingVO"
			alias="rtvo" />

		<typeAlias type="com.springprj.www.domain.LikeVO"
			alias="lvo" />
		<typeAlias type="com.springprj.www.domain.MovieVO"
			alias="mvvo" />

		<typeAlias type="com.springprj.www.domain.TVVO"
			alias="tvvo" />
		

	</typeAliases>
